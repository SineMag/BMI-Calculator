print('Please enter your weight in kg(s)')
w = int(input())
print("Now enter your height in m(s)")
h = float(input())
BMI = w/(h**2)

if BMI < 18.5:
    print(" You are nderweight")
elif BMI >= 18.5 and BMI < 25:
    print("You are normal")
elif BMI >= 25 and BMI < 30:
    print("You are overweight")
elif BMI > 30:
    print("You are obese")
